  char newInput[81];
  char reverseInput[81];
  int firstCounter = 0;
  for(int i = 0; i < strlen(input); i++){
    if(isalpha(input[i])){
      newInput[firstCounter] = input[i];
      firstCounter++;
    }
  }
  newInput[firstCounter] = '\0';

  for(int i = 0; i < strlen(newInput); i++){
    tolower(newInput[i]);
